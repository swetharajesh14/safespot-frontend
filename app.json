{
  "expo": {
    "name": "SafeSpot",
    "slug": "SafeSpot",
    "scheme": "safespot",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "SafeSpot needs your location to keep your Kin updated.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "SafeSpot protects you even when the app is closed.",
        "UIBackgroundModes": [
          "location",
          "fetch",
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.anonymous.SafeSpot",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "SEND_SMS",
        "CALL_PHONE",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "READ_CONTACTS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAf2DgOxVs7Zm2ueCNBHewI9mrl1C83adU"
        }
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98bb30ee-82e9-41ad-960e-a5e286026057"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/98bb30ee-82e9-41ad-960e-a5e286026057"
    }
  }
}
